query = f"""
SELECT DISTINCT
    sm.country,
    cc.code AS country_code,
    CASE
        WHEN sm.country = 'United States' THEN us.state_province
        ELSE 'Unknown'
    END AS state_province,
    sm.state AS state_code,
    sm.county,
    fa.territory_name,
    fa.zip_code,
    LOWER(SPLIT(fa.territory_name, ' ')[1]) AS region,
    sm.created_timestamp
FROM lh_200_silver.student_master sm
LEFT JOIN lh_200_silver.financial_aid fa ON sm.applicant_id = fa.applicant_id
LEFT JOIN us_states us ON sm.state = us.state
LEFT JOIN country_code cc ON sm.country = cc.country
"""